{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "apple-touch-icons": "off",
    "manifest-file-extension": "off",
    "axe/aria": [
      "default",
      {
        "aria-valid-attr-value": "off",
        "aria-allowed-attr": "off"
      }
    ]
  }
}